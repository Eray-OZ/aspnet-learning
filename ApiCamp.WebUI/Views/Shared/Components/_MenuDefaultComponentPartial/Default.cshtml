 
 
<!-- Menu Section -->



@model List<ApiCamp.WebUI.Dtos.ProductDtos.ResultProductWithCategoryDto>



<section id="menu" class="menu section">

    <!-- Section Title -->
    <div class="container section-title" data-aos="fade-up">
        <h2>Our Menu</h2>
        <p><span>Check Our</span> <span class="description-title">Yummy Menu</span></p>
    </div><!-- End Section Title -->

    <div class="container">

 
        <div class="tab-content" data-aos="fade-up" data-aos-delay="200">



                <div class="tab-pane fade active show" id="menu-starter">

                    <div class="tab-header text-center">
                        <p>Menu</p>
                        <h3>All</h3>
                    </div>

                    <div class="row gy-5">

                    @foreach (var item in Model)
                    {

                        <div class="col-lg-4 menu-item">
                            <a href="assets/img/menu/menu-item-1.png" class="glightbox"><img src="/Yummy/assets/img/menu/menu-item-1.png" class="menu-img img-fluid" alt=""></a>
                            <h4>@item.ProductName</h4>
                            <p class="ingredients">
                                @item.ProductDescription
                            </p>
                            <p class="price">
                                @item.Price
                            </p>
                            <h4>@item.CategoryName</h4>
                        </div><!-- Menu Item -->
                    }
                </div>
            </div><!-- End Starter Menu Content -->
            


        </div>

    </div>

</section><!-- /Menu Section -->


